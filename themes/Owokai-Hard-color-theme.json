    "tokenColors": [
        {
            "name": "Default Embedded Color",
            "scope": [
                "text",
                "source",
                "variable",
                "meta.macro",
                "keyword.symbol",
                "keyword.operator",
                "constant.character",
                "entity.name.variable",
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#dddddd",
                "fontStyle": ""
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#af9661",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.template",
                "punctuation.definition.string",
            ],
            "settings": {
                "foreground": "#4bbb5a"
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#6a95e6"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "entity.name.function.macro.rules.rust"
            ],
            "settings": {
                "foreground": "#d8b65a",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types",
            "scope": [
                "support",
                "storage",
                "entity.name",
                "keyword.type",
                "keyword.storage",
                "constant.other.placeholder",
                "punctuation.definition.interpolation"
            ],
            "settings": {
                "foreground": "#939799",
                "fontStyle": ""
            }
        },
        {
            "scope":[
                "entity.name.function.preprocessor",
                "entity.name.other.preprocessor.macro.predefined",
            ],
            "settings": {
                "fontStyle": "bold",
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#c00000",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Link",
            "scope": "string.detected-link",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#56b6c2A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Raw",
            "scope": [
                "markup.block.raw",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#939799"
            }
        }
    ],
    "semanticHighlighting": false
}
