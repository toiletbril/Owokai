    "tokenColors": [
        {
            "name": "Nothing",
            "scope": "",
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Comment",
            "scope": [
                "comment",
                "string.comment"
            ],
            "settings": {
                "foreground": "#7b785a",
                "fontStyle": "italic"
            }
        },
        {
            "name": "String",
            "scope": [
                "string",
                "string.template",
            ],
            "settings": {
                "foreground": "#59b165"
            }
        },
        {
            "name": "String Quotes",
            "scope": "punctuation.definition.string",
            "settings": {
                "foreground": "#59b165",
            }
        },
        {
            "name": "Number",
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#819fd7"
            }
        },
        {
            "name": "Default Embedded Color",
            "scope": [
                "punctuation.section.embedded.begin.erb",
                "punctuation.section.embedded.begin.js",
                "punctuation.section.embedded.end.erb",
                "punctuation.section.embedded.end.js",
                "storage.modifier.package",
                "storage.modifier.import",
                "source.elixir.embedded",
                "support.constant",
                "meta.brace",
                "source",
                "text",
            ],
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword.type",
                "keyword.other",
                "keyword.struct",
                "keyword.control",
                "keyword.import",
                "keyword.package",
                "keyword.function",
                "keyword.operator.logical",
                "keyword.operator.constructor"
            ],
            "settings": {
                "foreground": "#cdbe55",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keyword control",
            "scope": [
                "keyword.control"
            ],
            "settings": {
                "foreground": "#cdbe55",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Rust fix",
            "scope": [
                "storage.type.rust"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "name": "Constant",
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#888888",
            }
        },
        {
            "name": "User-defined constant",
            "scope": [
                "constant.character",
                "constant.other",
            ],
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Language Variable",
            "scope": "variable.language",
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Keyword Operator",
            "scope": "keyword.operator",
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Storage",
            "scope": [
                "storage.modifier",
                "storage.type.class",
                "storage.type.modifier.access"
            ],
            "settings": {
                "foreground": "#888888",
            }
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "entity.name.tag",
                "storage.type.namespace",
                "entity.name.tag.class.js",
                "constant.other.placeholder",
                "entity.other.inherited-class.java",
                "punctuation.definition.interpolation"
            ],
            "settings": {
                "foreground": "#888888",
                "fontStyle": ""
            }
        },
        {
            "name": "Class",
            "scope": [
                "support.class",
                "entity.name.type",
                "entity.name.class",
                "entity.name.tag.id",
                "entity.name.module",
                "storage.identifier",
                "entity.name.tag.class"
            ],
            "settings": {
                "foreground": "#888888"
            }
        },
        {
            "name": "Variable Object",
            "scope": [
                "variable.readwrite",
                "variable.other.object",
                "variable.other.global",
                "variable.other.constant",
                "entity.other.attribute-name",
                "variable.readwrite.other.block",
                "variable.other.readwrite.class",
                "variable.other.readwrite.instance",
                "variable.other.readwrite.batchfile",
            ],
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Other variable",
            "scope": [
                "variable.other",
                "variable.other.block",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Inherited Class",
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Function name",
            "scope": [
                "support.function",
                "meta.function.definition"
            ],
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Function call",
            "scope": [
                "meta.function.call",
                "entity.name.function-call"
            ],
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Function argument",
            "scope": [
                "variable.parameter",
                "entity.name.variable.parameter",
                "parameter.variable"
            ],
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Builtin Functions",
            "scope": [
                "function.support.core",
                "function.support.builtin"
            ],
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Library",
            "scope": "support",
            "settings": {
                "foreground": "#888888"
            }
        },
        {
            "name": "JSON String",
            "scope": "support.type.property-name.json",
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#c00000",
                "fontStyle": "strikethrough"
            }
        },
        {
            "name": "Link",
            "scope": "string.detected-link",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "diff.header",
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#75715E"
            }
        },
        {
            "name": "diff.deleted",
            "scope": "markup.deleted",
            "settings": {
                "foreground": "#e06c75"
            }
        },
        {
            "name": "diff.inserted",
            "scope": "markup.inserted",
            "settings": {
                "foreground": "#98c379"
            }
        },
        {
            "name": "diff.changed",
            "scope": "markup.changed",
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "scope": "constant.numeric.line-number.find-in-files - match",
            "settings": {
                "foreground": "#56b6c2A0"
            }
        },
        {
            "scope": "entity.name.filename.find-in-files",
            "settings": {
                "foreground": "#e5c07b"
            }
        },
        {
            "name": "General Punctuation",
            "scope": [
                "punctuation.comment",
                "punctuation.definition.bold.markdown",
                "punctuation.definition.italic.markdown",
                "punctuation.definition.heading.markdown",
            ],
            "settings": {
                "foreground": "#c5c5c5"
            }
        },
        {
            "name": "Dots and commas",
            "scope": [
                "punctuation.accessor",
                "punctuation.separator",
                "punctuation.terminator"
            ],
            "settings": {
                "foreground": "#c5c5c5",
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Bold",
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Markup: Heading",
            "scope": "markup.heading.markdown",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "name": "Markup: Raw",
            "scope": [
                "markup.block.raw",
                "markup.inline.raw"
            ],
            "settings": {
                "foreground": "#888888"
            }
        }
    ],
    "semanticHighlighting": false
}
