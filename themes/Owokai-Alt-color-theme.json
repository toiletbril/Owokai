  "tokenColors": [
    {
      "name": "Nothing",
      "scope": [
        ""
      ],
      "settings": {
        "foreground": "#c7ced6"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment",
        "punctuation.comment",
      ],
      "settings": {
        "foreground": "#586273",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "string.template",
      ],
      "settings": {
        "foreground": "#59b165"
      }
    },
    {
      "name": "String Quotes",
      "scope": "punctuation.definition.string",
      "settings": {
        "foreground": "#59b165",
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#b798f0"
      }
    },
    {
      "name": "Default Embedded Color",
      "scope": [
        "punctuation.section.embedded.begin.erb",
        "punctuation.section.embedded.begin.js",
        "punctuation.definition.interpolation",
        "punctuation.section.embedded.end.erb",
        "punctuation.section.embedded.end.js",
        "support.type.property-name.json",
        "constant.other.placeholder",
        "storage.modifier.package",
        "storage.modifier.import",
        "source.elixir.embedded",
        "variable.other.local",
        "meta.brace",
        "source",
        "text",
      ],
      "settings": {
        "foreground": "#c7ced6",
      }
    },
    {
      "name": "General Punctuation",
      "scope": [
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.terminator",
        "meta.separator.markdown",
        "punctuation.definition.tag",
        "punctuation.definition.list",
        "punctuation.definition.bold.markdown",
        "punctuation.definition.italic.markdown",
        "punctuation.definition.heading.markdown",
      ],
      "settings": {
        "foreground": "#7d8ca5"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant.language",
      ],
      "settings": {
        "foreground": "#b798f0"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "source.js-ignored-vscode",
        "constant.character",
        "constant.other",
      ],
      "settings": {
        "foreground": "#7d8ca5"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.name"
      ],
      "settings": {
        "foreground": "#c7ced6"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "keyword.type",
        "keyword.other",
        "keyword.struct",
        "keyword.control",
        "keyword.import",
        "keyword.package",
        "keyword.function",
        "keyword.operator",
        "entity.name.tag.css",
        "storage.type.function",
        "storage.type.annotation",
        "keyword.operator.constructor"
      ],
      "settings": {
        "foreground": "#77c4ff",
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#7d8ca5"
      }
    },
    {
      "name": "Storage modifiers",
      "scope": [
        "storage.modifier",
        "storage.type.enum",
        "storage.type.class",
        "storage.type.struct",
        "storage.type.namespace",
        "storage.type.modifier.access",
      ],
      "settings": {
        "foreground": "#77c4ff",
      }
    },
    {
      "name": "Types",
      "scope": [
        "keyword.var",
        "storage.type",
        "support.type",
        "keyword.type.cs",
        "entity.name.tag",
        "entity.name.tag.class.js",
        "entity.name.function-call",
        "entity.other.inherited-class.java",
      ],
      "settings": {
        "foreground": "#7d8ca5",
      }
    },
    {
      "name": "Class",
      "scope": [
        "support.class",
        "entity.name.type",
        "entity.name.class",
        "entity.name.tag.id",
        "entity.name.module",
        "storage.identifier",
        "entity.name.tag.class",
        "entity.name.namespace",
      ],
      "settings": {
        "foreground": "#7d8ca5"
      }
    },
    {
      "name": "Variable Object",
      "scope": [
        "variable.readwrite",
        "variable.language.this",
        "variable.readwrite.other.block",
        "variable.other.readwrite.class",
        "variable.other.readwrite.instance",
        "variable.other.readwrite.batchfile",
      ],
      "settings": {
        "foreground": "#c7ced6"
      }
    },
    {
      "name": "Other variable",
      "scope": [
        "variable.other.block",
        "variable.other.property",
        "variable.other.enummember",
        "entity.other.attribute-name.id",
        "variable.other.object.property",
        "entity.name.function.preprocessor",
      ],
      "settings": {
        "foreground": "#d06464"
      }
    },
    {
      "name": "Inherited Class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#c7ced6"
      }
    },
    {
      "name": "Function name",
      "scope": [
        "support.function",
        "entity.name.function",
        "variable.other.global",
        "meta.function.definition"
      ],
      "settings": {
        "foreground": "#dec25c",
      }
    },
    {
      "name": "Function argument",
      "scope": [
        "variable.parameter",
        "parameter.variable",
        "entity.name.variable.parameter"
      ],
      "settings": {
        "foreground": "#c7ced6",
      }
    },
    {
      "name": "Orange",
      "scope": [
        "function.support.core",
        "function.support.builtin",
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#c59440"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#b72222",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Link",
      "scope": "string.detected-link",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "diff.header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "foreground": "#75715E"
      }
    },
    {
      "name": "diff.deleted",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#d75a64"
      }
    },
    {
      "name": "diff.inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#63c96f"
      }
    },
    {
      "name": "diff.changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#7bc7e5"
      }
    },
    {
      "scope": "constant.numeric.line-number.find-in-files - match",
      "settings": {
        "foreground": "#56b6c2A0"
      }
    },
    {
      "scope": "entity.name.filename.find-in-files",
      "settings": {
        "foreground": "#e5c07b"
      }
    },
    {
      "name": "Markup: Emphasis Punctuation",
      "scope": "punctuation.definition.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Link",
      "scope": "markup.underline.link.markdown",
      "settings": {
        "foreground": "#5d99ca"
      }
    },
    {
      "name": "Markup: Bold",
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading.markdown",
      "settings": {
        "foreground": "#d06464",
      }
    },
    {
      "name": "Markup: Raw",
      "scope": [
        "markup.block.raw",
        "markup.inline.raw"
      ],
      "settings": {
        "foreground": "#7d8ca5"
      }
    }
  ],
  "semanticHighlighting": true
}
